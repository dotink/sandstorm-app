{
	"name": "inkwell/framework",
	"description": "The inKWell framework",
	"keywords": ["mvc", "framework", "application"],
	"license": "AGPL 3.0",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"authors": [
		{
			"name":  "Matthew J. Sahagian",
			"email": "msahagian@dotink.org",
			"role":  "Lead Architect / Developer"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.dotink.org"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"rdlowrey/auryn": "~1.0.0",
		"dotink/affinity": "~1.2.0",
		"dotink/inkwell-core": "~1.0.0",
		"vlucas/phpdotenv": "~2.0.0",
		"filp/whoops": "~2.1.0",
		"dotink/flourish-messenger": "dev-master",
		"dotink/inkwell-components": "dev-master",
		"dotink/inkwell-doctrine": "dev-master",
		"imarc/tenet": "~2.0@dev",
		"dotink/inkwell-auth": "dev-master",
		"dotink/inkwell-relay": "dev-master",
		"firebase/php-jwt": "^2.0.0",
		"twilio/sdk": "dev-master",
		"swiftmailer/swiftmailer": "^5.0.0",
		"cebe/markdown": "~1.1.0",
		"icanboogie/inflector": "^1.4"
	},
	"autoload": {
		"classmap": ["user/"]
	},
	"extra": {
		"opus": {
			"enabled": true
		}
	},
	"config": {
		"secure-http": false
	}
}
